package com.a1magway.bgg.data.entity;

import java.io.Serializable;
import java.util.List;

/**
 * author: Beaven
 * date: 2017/10/13 14:19
 */

public class MemberUpgradeInfo implements Serializable {

    private static final long serialVersionUID = 5437651458542034989L;

    private double sellPrice;
    private String roleDes;
    private int id;
    private double listPrice;
    private int roleType;
    private List<Permission> permissionList;
    private String registImagePath;

    public double getSellPrice() {
        return sellPrice;
    }

    public void setSellPrice(double sellPrice) {
        this.sellPrice = sellPrice;
    }

    public String getRoleDes() {
        return roleDes;
    }

    public void setRoleDes(String roleDes) {
        this.roleDes = roleDes;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public double getListPrice() {
        return listPrice;
    }

    public void setListPrice(double listPrice) {
        this.listPrice = listPrice;
    }

    public int getRoleType() {
        return roleType;
    }

    public void setRoleType(int roleType) {
        this.roleType = roleType;
    }

    public List<Permission> getPermission() {
        return permissionList;
    }

    public void setPermission(List<Permission> permissionList) {
        this.permissionList = permissionList;
    }

    public String getRegistImagePath() {
        return registImagePath;
    }

    public void setRegistImagePath(String registImagePath) {
        this.registImagePath = registImagePath;
    }
}
