package com.a1magway.bgg.data.entity;

import java.io.Serializable;
import java.util.List;

/**
 * 包装获取到的专题List的Model，用来区分专题类型
 * Created by jph on 2017/7/24.
 */
public class ActivityListWrapper implements Serializable {

    private static final long serialVersionUID = -4810707701830844903L;
    private List<ActivityData> activityList;
    /**
     * 对应{@link ActivityTypes}
     */
    private int type;

    public List<ActivityData> getActivityList() {
        return activityList;
    }

    public void setActivityList(List<ActivityData> activityList) {
        this.activityList = activityList;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }
}
