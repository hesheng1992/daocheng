package com.a1magway.bgg.data.entity;

import java.io.Serializable;
import java.util.List;

/**
 * 进入订单需要刷新数据，该类记录购物车选中结算项和购物车列表的d
 * Created by enid on 2018/7/27.
 */

public class AgainRequestCatDataTag implements Serializable {
    private List<Integer> lastCartIds;//购物车请求数据mlastCartId的集合
    private List<Integer> selectedIds;//购物车选中的ProductId集合

    public List<Integer> getLastCartIds() {
        return lastCartIds;
    }

    public void setLastCartIds(List<Integer> lastCartIds) {
        this.lastCartIds = lastCartIds;
    }

    public List<Integer> getSelectedIds() {
        return selectedIds;
    }

    public void setSelectedIds(List<Integer> selectedIds) {
        this.selectedIds = selectedIds;
    }
}
