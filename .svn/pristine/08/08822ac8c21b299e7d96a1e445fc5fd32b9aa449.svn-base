<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/text_black</item>
        <item name="android:windowBackground">@color/bg_page</item>
    </style>

    <style name="AppNewTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/black</item>
        <item name="android:windowBackground">@color/primaryColor</item>
        <item name="styleLoadingLayout">@style/LoadingLayoutStyle</item>
    </style>

    <style name="MyAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/black</item>
    </style>

    <style name="LoadingLayoutStyle" parent="LoadingLayout.Style">
        <item name="llEmptyResId">@layout/v_empty</item>
        <item name="llErrorResId">@layout/v_error</item>
    </style>

    <style name="TabBar_Tab" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TabBar_Tab_Found" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/text_black</item>
    </style>

    <style name="TextView.SingleLine" parent="android:Widget.TextView">
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="TextView.2Line" parent="android:Widget.TextView">
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="PageTabLayout" parent="Widget.Design.TabLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="tabTextAppearance">@style/Page_Tab</item>
        <item name="tabSelectedTextColor">@color/black</item>
    </style>

    <style name="PageTabLayoutHome" parent="Widget.Design.TabLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="tabIndicatorHeight">0dp</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="tabTextAppearance">@style/Page_Tab</item>
        <item name="tabSelectedTextColor">@color/white</item>
        <item name="tabTextColor">@color/black</item>
        <item name="tabBackground">@drawable/main_home_tab_bg</item>

    </style>

    <style name="Page_Tab" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/text_size_s</item>
        <item name="android:textColor">@color/text_grey</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!-- personal item -->
    <style name="Personal_Item" parent="android:Widget.TextView">
        <item name="android:drawableRight">@drawable/right_arrow_selector</item>
        <item name="android:textColor">@drawable/text_color_selector</item>
        <item name="android:background">@drawable/base_press_white_bg</item>
        <item name="android:paddingLeft">@dimen/personal_item_padding_left</item>
        <item name="android:paddingRight">@dimen/personal_item_padding_right</item>
        <item name="android:layout_height">@dimen/common_item_height</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="FullLine_View">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/common_divider_color</item>
    </style>

    <style name="Login_EditText_Style" parent="android:Widget.EditText">
        <item name="android:paddingLeft">@dimen/login_edt_padding_left</item>
        <item name="android:textSize">@dimen/text_size_m</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/login_edit_bg</item>
    </style>

    <style name="EditText_Style_Comm" parent="android:Widget.EditText">
        <item name="android:paddingLeft">@dimen/login_edt_padding_left</item>
        <item name="android:textSize">@dimen/text_size_m</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/edit_bg_comm</item>
    </style>

    <style name="Common_Btn_Style" parent="android:Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/enable_dark_corners_fourteen</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_l</item>
    </style>

    <style name="Search_Filter_Cate1" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/search_filter_cate1_bg</item>
    </style>

    <style name="Search_Filter_Cate2" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_s</item>
        <item name="android:textColor">@color/base_enable_grey_black</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:background">@drawable/search_filter_cate2_bg</item>
    </style>

    <style name="Personal_Setting_Item">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_item_height</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="Personal_Setting_Item_Left" parent="android:Widget.TextView">
        <item name="android:layout_width">105dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/text_size_m</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="PwdTipCheckBox" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">6dp</item>
        <item name="android:textSize">@dimen/text_size_s</item>
        <item name="android:textColor">@color/common_textColor_disable</item>
        <item name="android:layout_marginLeft">@dimen/login_edt_margin</item>
        <item name="android:layout_marginBottom">28dp</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@drawable/show_pwd_bg</item>
        <item name="android:text">@string/common_pwd_tip</item>
    </style>

    <style name="ConstrainLayout_Param_Full_Width">
        <item name="android:layout_width">0dp</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>

    <style name="alert_dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <!-- 自定义Dialog -->
    <style name="MNCustomDialog" parent="@android:style/Theme.Dialog">
        <!-- 无边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <!--无遮罩效果-->
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <declare-styleable name="MNProgressWheel">
        <attr name="mn_progressIndeterminate" format="boolean" />
        <attr name="mn_barColor" format="color" />
        <attr name="mn_rimColor" format="color" />
        <attr name="mn_rimWidth" format="dimension" />
        <attr name="mn_spinSpeed" format="float" />
        <attr name="mn_barSpinCycleTime" format="integer" />
        <attr name="mn_circleRadius" format="dimension" />
        <attr name="mn_fillRadius" format="boolean" />
        <attr name="mn_barWidth" format="dimension" />
        <attr name="mn_linearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MNCircularProgressBar">
        <attr name="mn_progress" format="integer" />
        <attr name="mn_progressbar_color" format="color" />
        <attr name="mn_background_progressbar_color" format="color" />
        <attr name="mn_progressbar_width" format="dimension" />
        <attr name="mn_background_progressbar_width" format="dimension" />
    </declare-styleable>

    <style name="anim_menu_bottombar">
        <item name="android:windowEnterAnimation">@anim/pop_in</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

</resources>
