package com.a1magway.bgg.v.productReturn.bean;

public class ReturnGoodsBean {
    private String imageUrl;
    private boolean isReturn;
    private String textReturn;
    private int refundID;

    public int getRefundID() {
        return refundID;
    }

    public void setRefundID(int refundID) {
        this.refundID = refundID;
    }

    public ReturnGoodsBean(){

    }

    public ReturnGoodsBean(String imageUrl, boolean isReturn, String textReturn) {
        this.imageUrl = imageUrl;
        this.isReturn = isReturn;
        this.textReturn = textReturn;
    }

    public String getTextReturn() {
        return textReturn;
    }

    public void setTextReturn(String textReturn) {
        this.textReturn = textReturn;
    }

    public String getImageUrl() {
        return imageUrl;
    }

    public void setImageUrl(String imageUrl) {
        this.imageUrl = imageUrl;
    }

    public boolean isReturn() {
        return isReturn;
    }

    public void setReturn(boolean aReturn) {
        isReturn = aReturn;
    }


}
