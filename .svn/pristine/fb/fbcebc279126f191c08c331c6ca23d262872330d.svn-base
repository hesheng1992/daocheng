package com.a1magway.bgg.data.entity;

import android.support.annotation.IntDef;

import java.lang.annotation.Retention;
import java.lang.annotation.RetentionPolicy;

import static com.a1magway.bgg.data.entity.PayType.ALIPAY;
import static com.a1magway.bgg.data.entity.PayType.UNION;
import static com.a1magway.bgg.data.entity.PayType.WECHAT;

/**
 * Created by jph on 2017/8/19.
 */
@Retention(RetentionPolicy.SOURCE)
@IntDef({ALIPAY, WECHAT, UNION})
public @interface PayType {
    int ALIPAY = 2;
    int WECHAT = 1;
    int UNION = 3;
}
