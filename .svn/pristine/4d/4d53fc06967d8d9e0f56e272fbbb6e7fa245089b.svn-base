public class SelectedCityEvent {
    private int cityId;
    private String cityName = "";
    private int grade = -1;//1级(0)，2级(2)，3级(2)

    public SelectedCityEvent(int grade, int cityId, String cityName) {
        this.grade = grade;
        this.cityId = cityId;
        this.cityName = cityName;
    }

    public int getCityId() {
        return cityId;
    }

    public String getCityName() {
        return cityName;
    }

    public int getGrade() {
        return grade;
    }
}
